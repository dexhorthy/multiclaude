# Makefile that already has setup/teardown targets
.PHONY: all build clean test setup teardown

all: build

setup:
	echo "Custom setup process"
	npm install
	npm run prepare

teardown:
	echo "Custom teardown process"
	npm run cleanup
	rm -rf node_modules/

build:
	npm run build

clean:
	rm -rf dist/

test:
	npm test